{
  "name": "deadfire-ai",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "description": "Create and edit Pillars of Eternity 2 - Deadfire AI scripts",
  "author": "SketchBuch - https://github.com/sketchbuch",
  "homepage": "./",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "build --em.main=build/electron.js",
    "flow": "flow",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "testc": "react-scripts test --env=jsdom --coverage",
    "testcw": "react-scripts test --env=jsdom --coverage --watch",
    "testv": "react-scripts test --env=jsdom --verbose"
  },
  "dependencies": {
    "binary-parser": "^1.3.2",
    "electron-is-dev": "^0.3.0",
    "md5": "^2.2.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.4.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.1.1",
    "recompose": "^0.30.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.2",
    "uuid": "^3.3.2",
    "uuid-parse": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "electron": "^1.8.7",
    "electron-builder": "^19.56.2",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-jest": "^21.21.0",
    "flow-bin": "^0.60.1",
    "redux-mock-store": "^1.5.3",
    "wait-on": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
